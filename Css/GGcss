// ==UserScript==
// @name         GoodCSS
// @namespace    new
// @version      0.1
// @description  :)
// @author       notPR
// @match        *://krunker.io/*
// @grant        none 
// @run-at       document-end
// ==/UserScript==

let topsheet = document.getElementsByTagName("link")[4];
let sheet = document.createElement("link");
sheet.setAttribute("href", "https://dl.dropbox.com/s/m8tcfaq9l0xmens/main.css?dl=0");
sheet.setAttribute("rel", "stylesheet");
topsheet.after(sheet);
